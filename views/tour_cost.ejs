                      document.getElementById('calc_cost_last').innerHTML = tempHtml;
                  }
                  document.getElementById('total_bgn_cost').innerHTML = addComma(bgnCost + parseInt(bgnCost/100*document.getElementById('input_profit_value_02').value));

                  document.getElementById('ins_profit_desc').style.display = "block";
              }

              if( document.getElementById('total_bgn_cost').innerHTML == "0" ) {
                  document.getElementById('btn_cost_detail_button').classList.add("disable");
              }
              else {
                  document.getElementById('btn_cost_detail_button').classList.remove("disable");
              }
